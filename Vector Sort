#include <bits/stdc++.h>
using namespace std;
int f(int x, int y)
{
    return x>y;
}
int main() 
{
    vector<int> v;
    for(int i=1;i<=5;i++)
    v.push_back(i*10);
    sort(v.begin(),v.end(),f);
    vector<int>::iterator it1;
    for(it1=v.begin();it1!=v.end();it1++)
    {
        cout<<*it1<<" ";
    }
	return 0;
}
**************************************************************
output
50 40 30 20 10 
