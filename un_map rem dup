#include <bits/stdc++.h> 
using namespace std; 

int main() 
{ 
   char str[]= "geeksforgeeks"; // input
  int n=sizeof(str);
  unordered_map<char,int> m;
  int i,index=0;
  for( i=0;i<n;i++)
  {
      if(m[str[i]]==0)
      {
          str[index++]=str[i];
          m[str[i]]++;
          //cout<<str[i];
      }
  }
  cout<<str;
   return 0; 
} 
